<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.finco.user.main.mapper.ExampleMapper">

    <select id="findById" parameterType="long" resultType="com.study.finco.user.main.model.vo.User">
        SELECT id, name, email
        FROM users
        WHERE id = #{id}
    </select>

</mapper>
